{
    "name": "dynamikaweb/yii2-dynamika-api",
    "description": "API Controller for dynamika projects",
    "type": "yii2-extension",
    "keywords": [
        "dynamika",
        "yii",
        "yii2",
        "api"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "RodrigoDornelles",
            "email": "rodrigo@dornelles.me",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "http://github.com/dynamikaweb/yii2-dynamika-api/issues",
        "source": "http://github.com/dynamikaweb/yii2-dynamika-api"
    },
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "codeception/verify": "^1.0",
        "codacy/coverage": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "dynamikaweb\\api\\": "src"
        }
    }
}
